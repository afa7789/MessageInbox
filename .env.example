# Example .env file for contract deployment
# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Network URLs
SEPOLIA_RPC=https://sepolia.infura.io/v3/your_infura_key
MAINNET_RPC=https://mainnet.infura.io/v3/your_infura_key

# Public encryption key for message contracts
# Replace with your actual public key
ENCRYPT_PUBLIC_KEY="-----BEGIN PGP PUBLIC KEY BLOCK-----
mQENBGH...your_real_pgp_key_here...
-----END PGP PUBLIC KEY BLOCK-----"

CONTRACT_TYPE="unsafe" 

# Etherscan API key for verification
ETHERSCAN_API_KEY=your_etherscan_api_key
